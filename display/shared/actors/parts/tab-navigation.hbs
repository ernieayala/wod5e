<nav class="sheet-tabs tabs"
    data-group="primary">
    {{#each tabs as |tab|}}
        <div class="item {{tab.cssClass}}"
            data-action="tab"
            data-group="{{tab.group}}"
            data-tab="{{tab.id}}"
            title="{{localize tab.title}}">
            {{localize tab.title}}
        </div>
    {{/each}}
</nav>
